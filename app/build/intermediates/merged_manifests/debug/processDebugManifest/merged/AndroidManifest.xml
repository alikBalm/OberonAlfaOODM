<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.alikbalm.oberon_alfaoodm"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="27" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name="com.orm.SugarApp"
        android:allowBackup="true"
        android:configChanges="orientation|screenSize"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.alikbalm.oberon_alfaoodm.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.alikbalm.oberon_alfaoodm.OOItemListActivity"
            android:configChanges="keyboardHidden|orientation|screenSize" />

        <meta-data
            android:name="DATABASE"
            android:value="sugar_orm_oo.db" />
        <meta-data
            android:name="VERSION"
            android:value="2" />
        <meta-data
            android:name="QUERY_LOG"
            android:value="true" />
        <meta-data
            android:name="DOMAIN_PACKAGE_NAME"
            android:value="com.alikbalm" />

        <activity
            android:name="com.alikbalm.oberon_alfaoodm.ReadSendEditMessages"
            android:configChanges="keyboardHidden|orientation|screenSize" />
        <activity
            android:name="com.alikbalm.oberon_alfaoodm.DocFilesReadWriteSaveDownload"
            android:configChanges="keyboardHidden|orientation|screenSize" />
    </application>

</manifest>